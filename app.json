{
  "name": "Voucherify React Demo Store - 'Hot Beans'",
  "description": "Set up your own Voucherify demo store to learn our API",
  "keywords": ["voucherify", "API"],
  "website": "https://www.voucherify.io",
  "repository": "https://github.com/rspective/voucherify-react-demo-store",
  "env": {
    "REACT_APP_BACKEND_APPLICATION_ID": {
      "description": "YOUR 'Application Keys' APPLICATION ID"
    },
    "REACT_APP_BACKEND_CLIENT_SECRET_KEY": {
      "description": "YOUR 'Application Keys' SECRET KEY"
    },
    "REACT_APP_FRONTEND_APPLICATION_ID": {
      "description": "YOUR 'Client-side Keys' APPLICATION ID"
    },
    "REACT_APP_FRONTEND_CLIENT_SECRET_KEY": {
      "description": "YOUR 'Client-side Keys' SECRET KEY"
    },
    "SESSION_SECRET": {
      "description": "Session secret password",
      "required": true
    },
    "API_PORT": {
      "description": "PORT",
      "value": 3000,
      "required": true
    }
  },
  "addons": [
    {
      "plan": "heroku-redis:hobby-dev"
    }
  ]
}